    "@types/testing-library__jest-dom": "^5.14.3",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.0.2",
    "babel-loader": "^9.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^8.2.0",
